    "event library",
    "ts events",
    "typescript events"
  ],
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "jasmine": "^3.6.1",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
