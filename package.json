  "description": "Type Safe EventEmitter",
  "main": "./dist/cjs/EventEmitter.js",
  "types": "./dist/cjs/EventEmitter.d.ts",
  "module": "./dist/esm/EventEmitter.js",
  "scripts": {
    "build:browser": "webpack",
    "build:commonjs": "tsc --project tsconfig.cjs.json",
